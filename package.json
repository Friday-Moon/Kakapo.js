{
  "name": "kakapo",
  "version": "1.0.3",
  "description": "Next generation mocking framework in Javascript",
  "main": "dist/index.js",
  "atlaskit:src": "src/index.js",
  "scripts": {
    "build": "babel -d dist src",
    "codecov": "make codecov",
    "readme": "node create-readme.js",
    "test": "jest",
    "test:ci": "jest --runInBand --coverage",
    "test:watch": "jest --watch",
    "amend": "git add . && git commit --amend --reuse-message=HEAD",
    "push": "git push --tags && git push",
    "prepublishOnly": "yarn build",
    "release": "yarn publish --silent --new-version patch && yarn push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devlucky/Kakapo.js.git"
  },
  "author": "devlucky",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devlucky/Kakapo.js/issues"
  },
  "homepage": "https://github.com/devlucky/Kakapo.js#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^5.4.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "5.4.0",
    "babel-plugin-__coverage__": "0.111111.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.8.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-flow": "^6.23.0",
    "babelify": "7.3.0",
    "browserify": "13.0.0",
    "codecov.io": "0.1.6",
    "eslint": "2.9.0",
    "eslint-config-airbnb-base": "2.0.0",
    "eslint-plugin-import": "1.7.0",
    "flow-bin": "^0.71.0",
    "ghooks": "1.2.1",
    "http-server": "~0.7.1",
    "jest": "^22.4.3",
    "jquery": "^2.2.4",
    "json-loader": "0.5.4",
    "npm-run-all": "1.7.0",
    "serve": "1.4.0",
    "superagent": "^2.0.0"
  },
  "dependencies": {
    "faker": "^3.1.0",
    "lodash": "4.10.0",
    "parse-url": "1.3.0",
    "path-match": "1.2.4",
    "path-to-regexp": "^1.2.1",
    "query-string": "4.1.0"
  },
  "keywords": [
    "mocking",
    "framework",
    "kakapo",
    "fetch",
    "XMLHttpRequest",
    "fake",
    "stub",
    "request"
  ],
  "files": [
    "dist"
  ]
}
