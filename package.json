{
  "name": "kakapo",
  "version": "1.0.2",
  "description": "Next generation mocking framework in Javascript",
  "main": "src/index.js",
  "scripts": {
    "build": "make build && git changelog",
    "readme": "node create-readme.js",
    "test": "jest",
    "test:ci": "jest --runInBand --coverage",
    "test:watch": "jest --watch",
    "amend": "git add . && git commit --amend --reuse-message=HEAD",
    "push": "git push --tags && git push",
    "release": "npm version patch && make build && npm run amend && npm run push && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devlucky/Kakapo.js.git"
  },
  "author": "devlucky",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devlucky/Kakapo.js/issues"
  },
  "homepage": "https://github.com/devlucky/Kakapo.js#readme",
  "devDependencies": {
    "babel-jest": "^22.4.3",
    "babel-preset-env": "^1.6.1",
    "http-server": "~0.7.1",
    "jest": "^22.4.3",
    "jquery": "^2.2.4",
    "json-loader": "0.5.4",
    "npm-run-all": "1.7.0",
    "serve": "1.4.0",
    "superagent": "^2.0.0"
  },
  "dependencies": {
    "faker": "^3.1.0",
    "lodash": "4.10.0",
    "parse-url": "1.3.0",
    "path-match": "1.2.4",
    "path-to-regexp": "^1.2.1",
    "query-string": "4.1.0"
  },
  "keywords": [
    "mocking",
    "framework",
    "kakapo",
    "fetch",
    "XMLHttpRequest",
    "fake",
    "stub",
    "request"
  ]
}
